{
	"server": {
		"port": 12024,
		"rateLimit": {
			"cooldownMs": 1000,
			"maxRequests": 2,
			"maxRequestLength": 128
		},
		"forceServeIncompatibleClients": false,
		"maxCommandHistorySaved": 256,
		"authentication": {
			"controllers": {
				"keys": [],
				"jwtExpiration": "1h"
			},
			"admins": {
				"keys": [],
				"jwtExpiration": "15m"
			},
			"jwtSecret": "",
			"crtPath": "certs/server.crt",
			"keyPath": "certs/server.key"
		}
	},
	"modules": {
		"wallpaper": {
			"public": {
				"isEnabled": true,
				"isDebug": false,
				"data": {
					"source": "https://localhost:12024/api/v3/image",
					"periodMs": 5000,
					"transitionMs": 1000
				}
			},
			"private": {
				"data": {
					"paths": [
						"./assets/123456.webp",
						"./assets/345612.webp",
						"./assets/561234.webp",
						"./assets/0.webp",
						"./assets/1.webp",
						"./assets/2.webp",
						"./assets/3.webp",
						"./assets/4.webp",
						"./assets/5.webp",
						"./assets/6.webp",
						"./assets/7.webp",
						"./assets/8.webp",
						"./assets/9.webp",
						"./assets/10.webp"
					]
				}
			}
		},
		"noise": {
			"public": {
				"isEnabled": false,
				"isDebug": false,
				"data": {
					"opacity": 0.5
				}
			}
		},
		"backdropFilter": {
			"public": {
				"isEnabled": true,
				"isDebug": false,
				"data": {
					"brightness": 0.5
				}
			}
		},
		"ripple": {
			"public": {
				"isEnabled": true,
				"isDebug": false,
				"data": {
					"width": "10px",
					"height": "10px",
					"position": "fixed",
					"borderRadius": "50%",
					"boxShadow": "0 0 4px 2px rgba(155,138,189,0.5)",
					"animation": "fx .6s ease forwards",
					"_keyframes":"fx{0%{transform:scale(0);}30%{opacity:1;}100%{transform:scale(20);opacity:0;}}"
				}
			}
		},
		"self": {
			"public": {
				"isEnabled": true,
				"isDebug": false,
				"data": {}
			}
		}
	}
}